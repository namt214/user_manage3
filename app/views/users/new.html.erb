<h1>New User</h1>
<form action="/users" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <p>
    <label for="first_name">First Name</label>
    <input id="first_name" type="text" name="user[first_name]">
  </p>
  <p>
    <label for="last_name">Last Name</label>
    <input id="last_name" type="text" name="user[last_name]" >
  </p>
  <p>
    <label for="email_address">Email Address</label>
    <input id="email_address" type="text" name="user[email_address]">
  </p>
  <p>
    <label for="password">password</label>
    <input id="password" type="text" name="user[password]">
  </p>
  <input type="submit" value="Create">
</form>
